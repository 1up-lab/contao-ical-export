{
  "name":"oneup/contao-ical-export",
  "description":"A contao module for downloading ical files generated from events. The output will follow RFC 2445 as best as possible.",
  "keywords":["contao", "ical", "icalendar", "export", "download"],
  "type":"contao-module",
  "homepage":"http://1up.io",
  "license":"MIT",
  "authors":[
    {
      "name":"David Greminger",
      "email":"dg@1up.io",
      "homepage":"http://1up.io",
      "role":"Developer"
    }
  ],
  "support":{
    "email":"hello@1up.io",
    "issues":"https://github.com/1up-lab/contao-ical-export/issues",
    "irc":"irc://irc.freenode.org/contao.dev",
    "source":"https://github.com/1up-lab/contao-ical-export"
  },
  "require":{
    "php":"^7.4 | ^8.0",
    "contao/core-bundle": "^4.9",
    "contao/calendar-bundle": "^4.9",
    "eluceo/ical": "^2.3"
  },
  "require-dev": {
    "contao/manager-plugin": "^2.0",
    "friendsofphp/php-cs-fixer": "^3.0",
    "phpstan/phpstan": "^1.0",
    "phpunit/phpunit": "9.5",
    "johngrogg/ics-parser": "^3.0"
  },
  "autoload": {
    "psr-4": {
      "Oneup\\Contao\\ICalExportBundle\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Oneup\\Contao\\ICalExportBundle\\Tests\\": "tests"
    }
  },
  "extra": {
    "contao-manager-plugin": "Oneup\\Contao\\ICalExportBundle\\ContaoManager\\Plugin"
  },
  "config": {
    "allow-plugins": {
      "contao-components/installer": true,
      "composer/package-versions-deprecated": true,
      "contao/manager-plugin": true
    }
  }
}
